{
  "name": "pocketnet-proxy-api",
  "version": "1.0.0",
  "description": "Library for interacting with the PocketNet proxy.",
  "author": "Pocketnet Team",
  "license": "Apache-2.0",
  "main": "./dist/lib/pocketnet-proxy.js",
  "types": "./dist/lib/pocketnet-proxy.d.ts",
